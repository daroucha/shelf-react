{
  "singleQuote": true,
  "semi": false,
  "printWidth": 70,
  "tabWidth": 2,
  "quoteProps": "as-needed",
  "trailingComma": "es5",
  "proseWrap": "preserve",
  "bracketSpacing": true,
  "arrowParens": "always",
  "endOfLine": "lf",
  "jsxSingleQuote": false,
  "importOrder": ["^@core/(.*)$", "^@server/(.*)$", "^@ui/(.*)$", "^[./]"],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true
}
